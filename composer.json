{
    "name": "bjdelange/propelrelationismodifiedbehavior",
    "description": "Propel behavior that extends the isModified method to support checking the related objects for changes.",
    "keywords": ["orm", "active record", "mapping", "database", "persistence"],
    "license": "MIT",
    "authors": [{
        "name": "Bert-Jan de Lange",
        "email": "info@bert-jan.com"
    }],
    "require": {
        "propel/propel1": "~1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "classmap": [
            "lib/"
        ],
        "psr-0": {
            "RelationIsModifiedBehavior": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
